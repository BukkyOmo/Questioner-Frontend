{
    "parser": "babel-eslint",
    "plugins": [
        "react"
      ],
    "rules": {
        "no-unused-vars": 0,
        "max-len": [1, 70, 2, {"ignoreComments": true}],
        "react/prop-types": [2],
        "comma-dangle": ["error", "never"]
    },
    "extends": ["airbnb-base"],
    "env": {
        "browser": true,
        "node": true
      },
        "globals": {
          "document": false
        }
}
