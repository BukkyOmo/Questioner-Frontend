{
    "name": "questioner-react",
    "version": "1.0.0",
    "description": "Questioner is an app that crowd-sources questions for meetups. It allows meetup organizers to prioritize the questions to be answered. Users of this platform can upvote or downvote a question. The questions however, are ranked according to their votes.",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production node app.js",
        "start:dev": "webpack-dev-server --open --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "heroku-postbuild": "npm run build",
        "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
        "test": "jest --runInBand",
        "test:watch": "jest --watchAll",
        "webpack": "webpack"
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BukkyOmo/Questioner-react.git"
    },
    "keywords": [
        "Questioner",
        "react",
        "redux",
        "testing",
        "axios"
    ],
    "author": "Bukola Omosefunmi",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/BukkyOmo/Questioner-react/issues"
    },
    "homepage": "https://github.com/BukkyOmo/Questioner-react#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "babel-jest": "^24.8.0",
        "babel-polyfill": "^6.26.0",
        "cors": "^2.8.5",
        "cross-env": "^5.2.0",
        "editorconfig": "^0.15.3",
        "express": "4.16.4",
        "jest-css-modules-transform": "^2.2.0",
        "jwt-decode": "^2.2.0",
        "prop-types": "^15.7.2",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-redux": "^7.0.3",
        "react-router-dom": "^5.0.0",
        "react-toastify": "^5.2.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "save": "^2.4.0",
        "shortid": "^2.2.14"
    },
    "devDependencies": {
        "@babel/core": "7.4.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "7.4.4",
        "@babel/preset-react": "7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "8.0.5",
        "babel-plugin-module-resolver": "^3.2.0",
        "clean-webpack-plugin": "^2.0.1",
        "coveralls": "3.0.3",
        "css-loader": "2.1.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.13.1",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.13.0",
        "eslint-watch": "^5.1.2",
        "file-loader": "3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.8.0",
        "moxios": "^0.4.0",
        "node-sass": "4.13.1",
        "react-hot-loader": "4.8.4",
        "redux-mock-store": "^1.5.3",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "url-loader": "1.1.2",
        "webpack": "4.30.0",
        "webpack-cli": "3.3.1",
        "webpack-dev-middleware": "3.6.2",
